@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@500&family=Poppins&display=swap');

.modal-overlay{
    display: none;
    position: fixed;
    z-index: 1;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    overflow: auto;
    background-color: rgba(0,0,0,0.5);
  }
  
  .modal-content{
    background-color: #f4f4f4;
    margin: 20% auto;
    width: 40%;
    box-shadow: 0 5px 8px 0 rgba(0,0,0,0.2),0 7px 20px 0 rgba(0,0,0,0.17);
  }
  
  .modal-header h2, .modal-footer h3{
    margin: 0;
  }
  
  .modal-header{
    
    padding: 15px;
    color: #fff;
  }
  
  .modal-body{
    padding: 10px 20px;
  }
  
  .modal-footer{
    background: #1ce;
    padding: 10px;
    color: #fff;
    text-align: center;
  }

  /* wallet */

  .connect {
      margin-top: -4px;
  }
 
  .submit {
      margin-top: 10px !important;
      cursor: pointer;
  }

  /* feeback after connection */

  .show {
    display: block;
    cursor: pointer;
    border: 1px soid red;
    box-shadow: 1px 1px 2px grey;
}

.alert {
    text-transform: capitalize;
    padding: 20px;
    background-color: #da2114;
    border-radius: 3px;
    display: flex;
    flex-direction: row;
    align-items: center;
    display: none;
    margin: 10px auto;
    width: 30vw;
}

.success {
    text-transform: capitalize;
    padding: 20px;
    background-color:   #04AA6D;
    color: whitesmoke;
    border-radius: 3px;
    display: flex;
    flex-direction: row;
    align-items: center;
    display: none;
    margin: 10px auto;
    width: 40vw;
}

.error_show{
    display: block;
}
.success_show {
    display: block;
}

.show {
    display: flex;
    flex-direction: row;
    background-color:  #04AA6D;
    color: white;
    text-transform: capitalize;
    font-size: 15px;
    width: 30vw;
    margin-left: 30px;
    margin-top: 20px;
}
  
.closebtn {
    margin-left: 15px;
    font-weight: bold;
    float: right;
    font-size: 18px !important;
    line-height: 20px;
    cursor: pointer;
    transition: 0.3s;
}

.closebtn:hover {
    color: black;
  }

.wallet span {
    background-color: #f1f1f1;;
    border-radius: 3px;
    color: black;
    font-size: 25px;
    text-transform: capitalize;
    box-shadow: 0px 1px 3px #6665ee;
    margin: 5px;
    width: 600px;
}


.center {
		  height:100%;
		  display:flex;
		  align-items:center;
		  justify-content:center;
		
		}
		.form-input {
		  
		  
		  width: 50%;
		  border-radius: 2px;
		  background:#fff;
		  box-shadow: -3px -3px 7px rgba(94, 104, 121, 0.377),
					  3px 3px 7px rgba(94, 104, 121, 0.377);
		}
		.form-input input {
		  display:none;
		
		}
		.form-input label {
		  display:block;
		  width:100%;
		  height:45px;
		  
		  line-height:50px;
		  text-align:center;
		  
		
		  color:#fff;
		  
		 
		  text-transform:Uppercase;
		  font-weight:600;
		  border-radius:5px;
		  cursor:pointer;
		}
		
		.form-input img {
		  width:100%;
		  border-radius: 2px;
		  display:none;
		
		  margin-bottom:30px;
		}
		
		   

